{
  "name": "demojockey",
  "displayName": "DemoJockey",
  "description": "",
  "version": "0.0.2",
  "publisher": "ptmfitch",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:demojockey.proofPoint"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "demojockey.proofPoint",
        "title": "Proof Point"
      }
    ],
    "configuration": {
      "title": "DemoJockey",
      "properties": {
        "demojockey.paths.pov-proof-exercises": {
          "description": "Absolute path of local pov-proof-exercises repository. (Hint: Output of running pwd in the root directory of the repository.)",
          "type": "string"
        },
        "demojockey.paths.tfw-micro-demos": {
          "description": "Absolute path of local tfw-micro-demos repository",
          "type": "string"
        },
        "demojockey.overwrite": {
          "description": "Whether to overwrite existing files when copying. (Default: false)",
          "type": "boolean",
          "default": false
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "esbuild": "^0.14.23",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4"
  }
}
